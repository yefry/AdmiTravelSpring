<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    @author Yeferson MarÃ­n
-->
<hibernate-mapping>
    <class name="co.edu.udea.ingenieriaweb.admitravel.dto.PaqueteDeViaje" table="paquetedeviaje">    
        <id name="idPaquete" type="string">
            <column name="IdPaquete" length="45"/>
        </id>
        <property name="destinos" type="string">
            <column name="Destinos" length="100" not-null="true"/>
        </property>
        <property name="transporte" type="string">
            <column name="Transporte" length="10" not-null="true"/>
        </property>        
        <property name="alimentacion" type="string">
            <column name="Alimentacion" length="10" not-null="true"/>
        </property>
        <property name="duracionViaje" type="string">
            <column name="DuracionViaje" length="30" not-null="true"/>
        </property>

        
    </class>
</hibernate-mapping>
